<a class="postPad postTitle" href="{{post_text}}" target="_blank">{{title}}</a>
<div class="interPostContainer">
<article class="postPad">
  <img class="userImg"src="{{user.user_img_url}}">
  <span>({{format_url post_text}})</span>
  <div class="row alignTextRight stayBetween">
    <a class="alignTextRight">by {{user.username}} on {{format_date created_at}}</a>
    |
    <a href="/post/{{id}}">{{comments.length}} {{format_plural "comment" comments.length}}</a>
  </div>
</article>
