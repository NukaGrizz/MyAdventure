<a class="searchHandlePostPad searchHandlePostTitle" href="/post/{{id}}" target="_blank">{{title}}</a>
<div class="searchHandleInterPostContainer">
<article class="searchHandlePostPad">
  <img class="searchHandleUserImg"src="{{user.user_img_url}}">
  <span>{{format_url post_text}}</span>
  <div class="row searchHandleAlignTextRight searchHandleStayBetween">
    <p class="searchHandleLabelMotto searchHandleAlignTextRight">by {{user.username}} on {{format_date created_at}}</p>
    |
    <a href="/post/{{id}}">{{comments.length}} {{format_plural "comment" comments.length}}</a>
  </div>

